{
    "name": "Famdirksen/laravel-exception-monitor",
    "description": "Little Laravel package to notify you about exceptions in your application.",
    "keywords": ["famdirksen", "laravel", "exception monitor"],
    "license": "MIT",
    "authors": [
        {
            "name": "Robin Dirksen",
            "email": "robin@famdirksen.nl"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "maknz/slack": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Famdirksen\\LaravelExceptionMonitor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Famdirksen\\LaravelExceptionMonitor\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit",
        "cs-fix": "php-cs-fixer fix . -vv || true"
    }
}
