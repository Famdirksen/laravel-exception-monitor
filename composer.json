{
    "name": "adriandmitroca/laravel-exception-monitor",
    "description": "Little Laravel package to notify you about exceptions in your application.",
    "keywords": ["adriandmitroca", "laravel", "exception monitor"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Dmitroca",
            "email": "dmitroca.adrian@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "maknz/slack": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {
            "Adriandmitroca\\LaravelExceptionMonitor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adriandmitroca\\LaravelExceptionMonitor\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit",
        "cs-fix": "php-cs-fixer fix . -vv || true"
    }
}
